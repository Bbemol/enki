{
  "version": 1,
  "projects": {
    "enki": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/enki",
      "sourceRoot": "apps/enki/src",
      "prefix": "fronts",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/enki",
            "index": "apps/enki/src/index.html",
            "main": "apps/enki/src/main.ts",
            "polyfills": "apps/enki/src/polyfills.ts",
            "tsConfig": "apps/enki/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/enki/src/favicon.ico",
              "apps/enki/src/assets"
            ],
            "styles": [
              "apps/enki/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/enki/src/environments/environment.ts",
                  "with": "apps/enki/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "enki:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "enki:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "enki:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/enki/src/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/enki/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "couv-ops": {
      "root": "apps/couv-ops",
      "sourceRoot": "apps/couv-ops/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/couv-ops",
            "index": "apps/couv-ops/src/index.html",
            "main": "apps/couv-ops/src/main.tsx",
            "polyfills": "apps/couv-ops/src/polyfills.ts",
            "tsConfig": "apps/couv-ops/tsconfig.app.json",
            "assets": [
              "apps/couv-ops/src/favicon.ico",
              "apps/couv-ops/src/assets"
            ],
            "styles": [
              "apps/couv-ops/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/couv-ops/src/environments/environment.ts",
                  "with": "apps/couv-ops/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "couv-ops:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "couv-ops:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/couv-ops/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/couv-ops/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-couv-ops": {
      "root": "libs/core-couv-ops",
      "sourceRoot": "libs/core-couv-ops/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core-couv-ops/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-couv-ops/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "utilities": {
      "root": "libs/utilities",
      "sourceRoot": "libs/utilities/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/utilities/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utilities/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "enki"
}

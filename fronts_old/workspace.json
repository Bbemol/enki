{
  "version": 1,
  "projects": {
    "couv-ops": {
      "root": "apps/couv-ops",
      "sourceRoot": "apps/couv-ops/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/couv-ops",
            "index": "apps/couv-ops/src/index.html",
            "main": "apps/couv-ops/src/main.tsx",
            "polyfills": "apps/couv-ops/src/polyfills.ts",
            "tsConfig": ["apps/couv-ops/tsconfig.app.json"],
            "assets": [
              "apps/couv-ops/src/favicon.ico",
              "apps/couv-ops/src/assets"
            ],
            "styles": ["apps/couv-ops/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/couv-ops/src/environments/environment.ts",
                  "with": "apps/couv-ops/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "couv-ops:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "couv-ops:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/couv-ops/tsconfig.app.json",
              "apps/couv-ops/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/couv-ops/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/couv-ops/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-couv-ops": {
      "root": "libs/core-couv-ops",
      "sourceRoot": "libs/core-couv-ops/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/core-couv-ops/tsconfig.lib.json",
              "libs/core-couv-ops/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-couv-ops/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-couv-ops/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  }
}
